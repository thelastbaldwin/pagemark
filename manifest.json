{	"name": "PageMark",
	"version": "1.0",   
	"description": "Tracks the last viewed location on a page",
	"browser_action": {     
		"default_icon": "icon.png",
		"default_title": "PageMark",
		"default_popup": "popup.html"
	},
	"content_scripts" :[
		{
			"matches" : ["<all_urls>"],
			"js" : ["scripts/jquery.1.9.1.min.js","scripts/contentscript.js"]
		}
	],
	"permissions" : ["storage", "tabs", "<all_urls>"],
	"manifest_version": 2,
	"offline_enabled": true
} 
